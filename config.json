{
    "workers": 0,
    "http": {
        "servers": [
            {
                "body-limit": 8192,
                "buffer-limit": 65535,
                "inactivity-timeout": 5000,
                "names": [
                    "localhost"
                ],
                "listen": [
                    "127.0.0.1:1234"
                ],
                "locations": {
                    "^/": {
                        "root": "/home/boat/CLionProjects/webserv-basic-async2/tests/html",
                        "index": [
                            "index.html",
                            "index.php"
                        ],
                        "allowed-methods": [
                            "GET"
                        ],
                        "autoindex": false
                    },
                    "^/dirtest/?": {
                        "root": "/home/boat/CLionProjects/webserv-basic-async2/ioruntime",
                        "index": [
                            "index.html",
                            "index.php"
                        ],
                        "locations": {
                            "\\.cpp$": {
                                "final": true,
                                "root": "/"
                            }
                        },
                        "allowed-methods": [
                            "GET"
                        ],
                        "autoindex": true
                    },
                    "^/cgi/?": {
                        "final": false,
                        "root": "/home/boat/CLionProjects/webserv-basic-async2/tests/cgi_test",
                        "allowed-methods": [
                            "GET",
                            "POST"
                        ],
                        "cgi": true,
                        "locations": {
                            ".+$": {
                                "final": true
                            }
                        },
                        "autoindex": true
                    }
                }
            }
        ]
    }
}