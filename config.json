{
    "workers": 4,
    "http": {
        "error_pages": {
            "404": "./tests/404.html"
        },
        "servers": [
            {
                "names": [
                    "localhost"
                ],
                "listen": [
                    "127.0.0.1:1234"
                ],
                "locations": {
                    "^/": {
                        "root": "/home/boat/CLionProjects/webserv-ssh/",
                        "index": [
                            "index.html",
                            "index.php"
                        ],
                        "allowed_methods": [
                            "GET"
                        ],
                        "autoindex": true
                    },
                    "^/dirtest/?": {
                        "root": "/home/boat/CLionProjects/webserv-ssh/ioruntime",
                        "index": [
                            "index.html",
                            "index.php"
                        ],
                        "locations": {
                            "\\.cpp$": {
                                "final": true,
                                "root": "/",
                                "cgi": true
                            }
                        },
                        "allowed_methods": [
                            "GET"
                        ],
                        "autoindex": true
                    }
                }
            }
        ]
    }
}