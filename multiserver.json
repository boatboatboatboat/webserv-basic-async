{
    "workers": 0,
    "http": {
        "error-pages": {
            "404": "../html/index.html"
        },
        "servers": [
            {
                "body-limit": 1000000000,
                "buffer-limit": 1000000000,
                "timeout": 40000000,
                "root": ".",
                "listen": [
                    "127.0.0.1:1236"
                ],
                "locations": {
                    "^//?$": {
                        "root": "./tests/youpi_tester/YoupiBanane/index.html",
                        "allowed-methods": [
                            "GET"
                        ]
                    },
                    "^/put_test/?": {
                        "allowed-methods": [
                            "PUT"
                        ],
                        "upload": {
                            "directory": "./output"
                        }
                    },
                    "^/post_body/?": {
                        "root": "./tests/youpi_tester/YoupiBanane/index.html",
                        "allowed-methods": [
                            "POST"
                        ],
                        "body-limit": 100
                    },
                    "^/directory/?": {
                        "root": "./tests/youpi_tester/YoupiBanane",
                        "index": [
                            "youpi.bad_extension"
                        ],
                        "locations": {
                            "\\.bla$": {
                                "root": "./tests/youpi_tester/ubuntu_cgi_tester",
                                "allowed-methods": [
                                    "GET",
                                    "POST"
                                ],
                                "final": false,
                                "cgi": true
                            }
                        }
                    }
                }
            },
            {
                "names": ["localhost"],
                "listen": [
                    "127.0.0.1:1235"
                ],
                "locations": {
                    "^/$": {
                        "root": "/home/boat/CLionProjects/webserv-basic-async2/tests/html",
                        "autoindex": true
                    }
                }
            },
            {
                "body-limit": 8192,
                "buffer-limit": 65535,
                "timeout": 5000,
                "names": [
                    "localhost"
                ],
                "listen": [
                    "127.0.0.1:1234"
                ],
                "locations": {
                    "^/": {
                        "root": "/home/boat/CLionProjects/webserv-basic-async2/tests/html",
                        "index": [
                            "index.html",
                            "index.php"
                        ],
                        "allowed-methods": [
                            "GET"
                        ],
                        "autoindex": false
                    },
                    "^/authtest/?": {
                        "root": "/home/boat/CLionProjects/webserv-basic-async2/ioruntime",
                        "auth": {
                            "username": "hello",
                            "password": "world",
                            "realm": "\\Weird\"Realm"
                        },
                        "autoindex": true
                    },
                    "^/dirtest/?": {
                        "root": "/home/boat/CLionProjects/webserv-basic-async2/ioruntime",
                        "index": [
                            "index.html",
                            "index.php"
                        ],
                        "locations": {
                            "\\.cpp$": {
                                "final": true,
                                "root": "/"
                            }
                        },
                        "allowed-methods": [
                            "GET"
                        ],
                        "autoindex": true
                    },
                    "^/cgi/?": {
                        "final": false,
                        "root": "/home/boat/CLionProjects/webserv-basic-async2/tests/cgi_test",
                        "allowed-methods": [
                            "GET",
                            "POST"
                        ],
                        "cgi": true,
                        "locations": {
                            ".+$": {
                                "final": true
                            }
                        },
                        "autoindex": true
                    }
                }
            }
        ]
    }
}